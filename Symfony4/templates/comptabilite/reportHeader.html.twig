<!-- En tete du rapport-->
<div class="row border rounded p-2">
    <div class="col-4">
        <span>{{mutuelle.nom_mutuelle}}</span><br>
        <span>{{mutuelle.adresse_mutuelle}}</span><br>
        <span>{{mutuelle.contact_mutuelle}}</span><br>
        <span>{{mutuelle.email_mutuelle}}</span><br>
    </div>
    <div class="col-4 text-center pt-4">
        <h2 class="titre-rapport">{{ title|capitalize }}</h2>
         {% if subtitle is defined %}<span>{{subtitle}}</span>{% endif %}  
    </div>
    <div class="col-4 text-right">
        <br>
        {% if periode is defined %}
        <span>Période du {{ periode.debut|date('d/m/Y') }}</span><br>
        <span>au {{ periode.fin|date('d/m/Y') }}</span> <br>
        {% else %}
        <span>Période du {{ app.session.get('exercice').dateDebut|date('d/m/Y') }}</span> <br>
        <span>au {{ app.session.get('exercice').dateFin|date('d/m/Y') }}</span> <br>
        {% endif %}
        <span>Tenue du compte : Ar </span> <br>
    </div>
</div>